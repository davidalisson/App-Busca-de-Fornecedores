<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myproject.com.fornec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CadastroClienteActivity" />
        <activity android:name=".Activity.CadastroFornecedorActivity" />
        <activity android:name=".Activity.OpcaoCadastroActivity" />
        <activity
            android:name=".Activity.BuscaFornecedorActivity"
            android:label="@string/title_activity_lista_fornecedor"></activity>
        <activity
            android:name=".Activity.PrincipalClienteActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.PrincipalFornecedorActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.ListarFornecedorActivity"
            android:label="@string/title_activity_lista_fornecedor"></activity>
        <activity
            android:name=".Activity.PerfilFornecedorActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.ChamadaOrcamentoActivity" />
        <activity android:name=".Activity.PerfilChamadaOrcamentoActivity" />
        <activity android:name=".Activity.AvaliacaoActivity"></activity>
    </application>

</manifest>